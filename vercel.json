{
  "version": 2,
  "builds": [
    {
      "src": "build/web/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/build/web/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/build/web/$1"
    }
  ],
  "functions": {},
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/build/web/index.html"
    }
  ]
}